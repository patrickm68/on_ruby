section
  h1= title

section
  ul
    - Whitelabel.labels.each do |label|
      - if label.enabled?
        li
          - name = t("label.#{label.label_id}.name")
          = link_to label_url(label), title: name do
            span.image = image_tag("labels/#{label.label_id}.png", alt: name)
            span.host  = name
